{
	"source": "/Volumes",
	"source_ignore": ["Backup", "Macintosh HD"],
	"destination": "~/Movies/Movies",
	"cache": "cache",
	"handbrake": "/usr/local/bin/HandBrakeCLI",
	"atomicparsley": "/usr/local/bin/AtomicParsley",
	"caffeinate": "/usr/bin/caffeinate",
	"eject": "/usr/sbin/diskutil eject",
	"mount": "/usr/sbin/diskutil mount",
	"tmdb_key": "YOUR_KEY"
}